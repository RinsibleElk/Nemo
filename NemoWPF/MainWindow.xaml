<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:ViewModels;assembly=NemoWpf"
        Icon="nemo.ico"
        Title="NemoWPF" Height="750" Width="1200">
    <Window.Resources>
        <ResourceDictionary>
            <local:MainViewModel x:Key="model" />
        </ResourceDictionary>
    </Window.Resources>
    <Window.DataContext>
        <Binding Source="{StaticResource model}" />
    </Window.DataContext>
    <DockPanel LastChildFill="True">
    </DockPanel>
</Window>
